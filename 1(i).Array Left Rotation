/*
check on the link
https://www.hackerrank.com/challenges/array-left-rotation/problem?isFullScreen=true
Sample Input

5 4
1 2 3 4 5
Sample Output d=2(after two rotate)

5 1 2 3 4

*/

#include<bits/stdc++.h>
using namespace std;
void reverseONE(int arr[],int n)
{
    int temp=arr[0],i;
    for( i=0;i<n-1;i++)
      arr[i]=arr[i+1];
        arr[i]=temp;
    

}
void reverse(int a[],int d,int n)
{
    for(int i=0;i<d;i++)
    {
      reverseONE(a,n);
    }
}
void print(int n,int arr[])
{
    for(int i=0;i<n;i++)
    {
        cout<<arr[i]<<" ";
    }
    cout<<endl;
 }
int main()
{
int n,m;
cin>>n>>m;
int a[n];
for(int i=0;i<n;i++)
{
    cin>>a[i];
}
reverse(a,m,n);
print(n,a);
return 0;
}
